{
  "name": "vueisotope",
  "version": "3.1.2",
  "description": "vue isotope for directive",
  "main": "dist/vueisotope.umd.js",
  "module": "dist/vueisotope.esm.js",
  "unpkg": "dist/vueisotope.min.js",
  "browser": {
    "./sfc": "src/vueisotope.vue"
  },
  "keywords": [
    "vue",
    "vuejs",
    "isotope",
    "web-components",
    "web-component",
    "layout"
  ],
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "dependencies": {
    "isotope-layout": "^3.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "postcss": "^7.0.27",
    "rollup": "^1.32.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue-template-compiler": "^2.6.11"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vueisotope.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vueisotope.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vueisotope.min.js"
  },
  "author": "David Desmaisons",
  "bugs": {},
  "license": "MIT",
  "homepage": " https://github.com/David-Desmaisons/Vue.Isotope#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/David-Desmaisons/Vue.Isotope.git"
  }
}
